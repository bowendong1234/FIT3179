{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "vconcat": [
        {
            "width": 400,
            "height": 250,
            "title": "Average PBS scripts per state for inhalants and adrenergics for obstructive airway diseases",
            "data": {
                "url": "Inhalants_and_adrenergics.csv",
                "format": {
                    "type": "csv"
                }
            },
            "transform": [
                {
                    "fold": [
                        "VIC",
                        "NSW",
                        "QLD",
                        "ACT",
                        "NT",
                        "SA",
                        "WA",
                        "TS"
                    ],
                    "as": [
                        "State",
                        "Value"
                    ]
                }
            ],
            "layer": [
                {
                    "mark": {
                        "type": "line"
                    },
                    "encoding": {
                        "x": {
                            "field": "Week",
                            "type": "ordinal",
                            "title": "Weeks (September 2019 - February 2020)",
                            "sort": [
                                "1/9/2019",
                                "8/9/2019",
                                "15/9/2019",
                                "22/9/2019",
                                "29/9/2019",
                                "6/10/2019",
                                "13/10/2019",
                                "20/10/2019",
                                "27/10/2019",
                                "3/11/2019",
                                "10/11/2019",
                                "17/11/2019",
                                "24/11/2019",
                                "1/12/2019",
                                "8/12/2019",
                                "15/12/2019",
                                "22/12/2019",
                                "29/12/2019",
                                "5/1/2020",
                                "12/1/2020",
                                "19/1/2020",
                                "26/1/2020",
                                "2/2/2020",
                                "9/2/2020",
                                "16/2/2020",
                                "23/2/2020"
                            ]
                        },
                        "y": {
                            "field": "Value",
                            "type": "quantitative",
                            "title": "Rate change in no. of scripts ordered (%)",
                            "scale": {
                                "zero": true
                            }
                        },
                        "color": {
                            "field": "State",
                            "type": "nominal",
                            "title": "State"
                        },
                        "tooltip": [
                            {
                                "field": "State",
                                "type": "nominal",
                                "title": "State"
                            },
                            {
                                "field": "Value",
                                "type": "quantitative",
                                "title": "% change in prescriptions ordered"
                            },
                            {
                                "field": "Week",
                                "type": "nominal",
                                "title": "Week starting"
                            }
                        ]
                    }
                },
                {
                  "mark": {
                    "type": "text",
                    "align": "right",
                    "dx": 5,
                    "dy": -5,
                    "font": "calibri",
                    "fontWeight": "lighter",
                    "fontSize": 12
                  },
                  "encoding": {
                    "x": {
                      "datum": "12/1/2020",
                      "type": "ordinal"
                    },
                    "y": {
                      "datum": 27.3,
                      "type": "quantitative"
                    },
                    "text": {
                      "value": "surge in prescriptions\ndue to 2020 black summer"
                    }
                  }
                },
                {
                  "mark": {
                    "type": "rule",
                    "strokeDash": [1,3],
                    "strokeWidth": 1
                },
                  "encoding": {
                    "x": {
                      "datum": "12/1/2020",
                      "type": "ordinal"
                    },
                    "y": {
                      "value": 0
                    }
                  }
                }
            ]
        }
    ],
    "config": {
        "view": {
            "stroke": null
        }
    }
}